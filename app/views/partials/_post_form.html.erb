<div class="row">
<div class="col-md-8 col-md-offset-2">
<%#= This partial takes in/creates city and post information fields for user input %>
  <%= form_for [@city, @post], class: "form-group" do |f|%>
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
    <br/>
    <%= f.label :content %>
    <%= f.text_area :content, class: "form-control", style: "height: 300px;" %>
    <br/>
    <%= f.submit(:class => "btn btn-warning") %>
  <% end %>
</div>
</div>